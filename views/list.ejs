<%- include('header') -%>
<div class="box" id="heading">
    <h1><%= listTitle %></h1>
</div>

<div class="box">
    <ul>

        <% for (let i = 0;i < newListItems.length; i++) { %>
        <div class="item">
            <input type="checkbox">
            <p>
                <%= newListItems[i] %>
            </p>
        </div>

        <% } %>

    </ul>
    <form class="item" action="/" method="post">
        <input autocomplete="off" name="item" type="text" placeholder="New item">
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
</div>
<%- include('footer') -%>

